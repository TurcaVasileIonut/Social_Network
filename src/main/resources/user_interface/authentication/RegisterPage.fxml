<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="mainPane" prefHeight="511.0" prefWidth="482.0" stylesheets="@../css/register_page.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="user_interface.authentication.RegisterPageController">
    <children>
        <TextField fx:id="firstname" layoutX="138.0" layoutY="103.0" prefHeight="26.0" prefWidth="233.0" promptText="Firstname">
            <font>
                <Font name="Arial Narrow Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="lastname" layoutX="138.0" layoutY="147.0" prefHeight="26.0" prefWidth="234.0" promptText="Lastname">
            <font>
                <Font name="Arial Narrow Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="email" layoutX="138.0" layoutY="192.0" prefHeight="26.0" prefWidth="234.0" promptText="Email">
            <font>
                <Font name="Arial Narrow Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="username" layoutX="138.0" layoutY="243.0" prefHeight="26.0" prefWidth="234.0" promptText="Username">
            <font>
                <Font name="Arial Narrow Italic" size="12.0" />
            </font></TextField>
        <PasswordField fx:id="password" layoutX="137.0" layoutY="288.0" prefHeight="26.0" prefWidth="234.0" promptText="Password">
            <font>
                <Font name="Arial Narrow Italic" size="12.0" />
            </font>
        </PasswordField>
        <PasswordField fx:id="confirmPassword" layoutX="139.0" layoutY="340.0" prefHeight="26.0" prefWidth="234.0" promptText="Confirm Password">
            <font>
                <Font name="Arial Narrow Italic" size="12.0" />
            </font>
        </PasswordField>
        <ComboBox fx:id="gender" layoutX="138.0" layoutY="393.0" prefHeight="26.0" prefWidth="234.0" promptText="Gender" />
        <Button id="main_button" fx:id="buttonRegister" layoutX="139.0" layoutY="432.0" mnemonicParsing="false" onAction="#registerClick" prefHeight="28.0" prefWidth="234.0" text="Create Account" textFill="#3383ff">
            <font>
                <Font name="Arial Black" size="13.0" />
            </font>
        </Button>
      <Hyperlink fx:id="hyperlinkRegister" layoutX="294.0" layoutY="466.0" onAction="#loginClick" prefHeight="28.0" prefWidth="62.0" text="Login">
         <font>
            <Font name="Arial Rounded MT Bold" size="13.0" />
         </font></Hyperlink>
      <Label layoutX="171.0" layoutY="466.0" prefHeight="28.0" prefWidth="122.0" text="Already registered?">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Label>
      <Label layoutX="147.0" layoutY="14.0" prefHeight="71.0" prefWidth="214.0" text="Socialio" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="48.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
